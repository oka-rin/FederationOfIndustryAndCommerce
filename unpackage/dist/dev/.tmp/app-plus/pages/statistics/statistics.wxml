<view class="page" style="background-color:#FFFFFF;"><view class="ring-row" style="height:100rpx;padding:10rpx 0;background-color:#EEEEED;justify-content:space-between;"><view class="ring-row"><image style="border-radius:50%;width:80rpx;margin-left:20rpx;" src="../../static/tou.png" mode="widthFix"></image><view>张明轩</view></view><view style="display:flex;flex-direction:row;align-items:center;"><view class="tui-drop-input-box"><tui-dropdown-list vue-id="1" show="{{oneShow}}" top="{{94}}" height="{{400}}" bind:__l="__l" vue-slots="{{['selectionbox','dropdownbox']}}"><view slot="selectionbox"><view data-event-opts="{{[['tap',[['getOneList',[-1]]]]]}}" class="time-button" bindtap="__e">{{oneName+''}}<view class="{{['tui-animation',oneShow?'tui-animation-show':'']}}"><tui-icon vue-id="{{('2')+','+('1')}}" name="turningdown" size="{{20}}" bind:__l="__l"></tui-icon></view></view></view><view slot="dropdownbox"><view class="tui-selected-list"><scroll-view class="tui-dropdown-scroll" scroll-y><block wx:for="{{oneList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><tui-list-cell vue-id="{{('3-'+index)+','+('1')}}" last="{{oneList.length-1==index}}" data-event-opts="{{[['^click',[['getOneList',[index]]]]]}}" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}"><text class="tui-ml-20">{{item.name}}</text></tui-list-cell></block></block></scroll-view></view></view></tui-dropdown-list></view><view class="tui-drop-input-box"><tui-dropdown-list vue-id="4" show="{{towShow}}" top="{{94}}" height="{{400}}" bind:__l="__l" vue-slots="{{['selectionbox','dropdownbox']}}"><view slot="selectionbox"><view data-event-opts="{{[['tap',[['dropDownList',[-1]]]]]}}" class="time-button" bindtap="__e">{{towName+''}}<view class="{{['tui-animation',towShow?'tui-animation-show':'']}}"><tui-icon vue-id="{{('5')+','+('4')}}" name="turningdown" size="{{20}}" bind:__l="__l"></tui-icon></view></view></view><view slot="dropdownbox"><view class="tui-selected-list"><scroll-view class="tui-dropdown-scroll" scroll-y><block wx:for="{{towList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><tui-list-cell vue-id="{{('6-'+index)+','+('4')}}" last="{{towList.length-1==index}}" data-event-opts="{{[['^click',[['dropDownList',[index]]]]]}}" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}"><text class="tui-ml-20">{{item.name}}</text></tui-list-cell></block></block></scroll-view></view></view></tui-dropdown-list></view></view></view><view class="qiun-columns"><view class="qiun-charts"><canvas class="charts" canvas-id="canvasRing" id="canvasRing" data-event-opts="{{[['touchstart',[['touchRing',['$event']]]]]}}" bindtouchstart="__e"></canvas></view></view><view class="ring-block"><block wx:for="{{chartData.series}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="ring-item"><view class="ring-row"><view class="ring-color" style="{{'background:'+(item.color)+';'}}"></view><view style="margin-left:10px;line-height:20px;">{{item.name}}</view></view><view class="ring-row" style="margin-top:10px;font-size:12px;margin-left:5px;"><view class="ring-point"></view><view style="margin-left:10px;line-height:8px;">{{item.data+"次"}}</view></view><view class="ring-row" style="margin-top:10px;font-size:12px;margin-left:5px;"><view class="ring-point"></view><view style="margin-left:10px;line-height:8px;">用时24小时</view></view></view></block></view></view>