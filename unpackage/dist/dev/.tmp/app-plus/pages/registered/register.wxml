<view class="page"><view class="register-ico"><image style="width:80px;border-radius:50%;" src="../../static/test/gs03.jpg" mode="widthFix"></image></view><view style="margin-top:50px;width:80%;padding:0px 10%;"><form data-event-opts="{{[['submit',[['formSubmit']]]]}}" bindsubmit="__e"><view><view style="text-align:center;font-size:14px;color:#FF0000;margin-bottom:5px;">{{prompt}}</view><input style="height:60px;" type="number" placeholder="请输入手机号" data-event-opts="{{[['input',[['__set_model',['','phone','$event',[]]]]]]}}" value="{{phone}}" bindinput="__e"/><view class="registerfg"></view><view style="display:flex;flex-direction:row;"><input style="height:60px;width:100%;" type="number" placeholder="请输入验证码" data-event-opts="{{[['input',[['__set_model',['','verification','$event',[]]]]]]}}" value="{{verification}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['getMsg']]]]}}" class="verification" bindtap="__e">{{msg}}</view></view><view class="registerfg"></view><input style="height:60px;" type="text" placeholder="请输入登录密码" password data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/><view class="registerfg"></view><input style="height:60px;" type="text" placeholder="请再次输入登录密码" password data-event-opts="{{[['input',[['__set_model',['','netpassword','$event',[]]]]]]}}" value="{{netpassword}}" bindinput="__e"/><view class="registerfg"></view></view><view style="margin-top:40px;"><button type="primary" form-type="submit">注册</button></view></form></view></view>