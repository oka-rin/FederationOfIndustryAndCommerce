.swiper-area{
	width: 100%;
	height: 100%;
	background-color: #EEEEED;
	position:-webkit-sticky;
	position:sticky;
	margin-top: 100rpx;
}
.schedule-block{
	background-color: #FFFFFF;
	padding: 20rpx 0;
	width: 100%;
	border-top: #EEEEED solid 1rpx;
}
.schedule-btn{
	background-color: #EEEEED;
	border-radius: 20px;
	padding:5rpx 20rpx;
	font-size: 12px;
}
.schedule-pos-item{
	background-color: #FFFFFF;
	border-radius: 10rpx;
	width: 94%;
	padding: 5% 3%;
}

